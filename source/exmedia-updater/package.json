  "main": "build/electron.js",
  "scripts": {
    "dev": "nf start -p 3000",
    "start": "node server",
    "start-dev": "node scripts/customized-config start",
    "start-hot-reload": "cross-env NODE_ENV=dev nodemon --watch ./src/**/* --watch . --exec 'yarn electron'",
    "build-dev": "cross-env REACT_APP_STAGE=stag node scripts/customized-config build",
    "build": "node --max_old_space_size=8000 scripts/customized-config build",
    "test": "yarn test:flow && node scripts/customized-config test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "yarn build-dev && yarn electron",
    "electron-web-dev": "concurrently \"BROWSER=none yarn start-dev\" \"wait-on http://localhost:3000 && ELECTRON_START_URL=http://localhost:3000 electron .\"",
    "postinstall": "build install-app-deps",
    "electron-pack:mac": "build --mac -c.extraMetadata.main=build/electron.js",
    "electron-pack:win": "build --win -c.extraMetadata.main=build/electron.js",
    "electron-pack:linux": "build --linux -c.extraMetadata.main=build/electron.js",
    "publish:mac": "cross-env GH_TOKEN=36aeee759e974840cbcd481e3ea8325216609d28 build --mac --publish always -c.extraMetadata.main=build/electron.js",
    "publish:win": "cross-env GH_TOKEN=36aeee759e974840cbcd481e3ea8325216609d28 build --win --publish always -c.extraMetadata.main=build/electron.js",
    "publish:linux": "cross-env GH_TOKEN=36aeee759e974840cbcd481e3ea8325216609d28 build --linux --publish always -c.extraMetadata.main=build/electron.js",
    "make-cert": "build create-self-signed-cert -p ptgps",
    "pack": "yarn electron-pack:mac && yarn electron-pack:win && yarn electron-pack:linux",
    "dist": "yarn publish:mac && yarn publish:win && yarn publish:linux",
    "test:flow": "node_modules/flow-bin/cli.js --show-all-errors"
  }
}
